{
  "private": false,
  "license": "Apache-2.0",
  "version": "0.0.0",
  "name": "@oxygen/discord-backend",
  "description": "Design System by Rafcin",
  "author": {
    "name": "Raf Szuminski",
    "email": "rafcin.s@gmail.com",
    "url": "https://rafszuminski.com"
  },
  "repository": "rafcin/oxygen.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "backend",
    "prisma",
    "discord",
    "mongodb",
    "db",
    "trpc"
  ],
  "engines": {
    "node": "^14.13.1 || >=16.0.0",
    "pnpm": ">=7"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css",
    "./primitives.json": "./dist/primitives.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "[--- Build ---]": "",
    "prebuild": "rimraf dist",
    "build": "pnpm prisma:generate && pnpm run build:ts",
    "build:ts": "rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "[--- Dev ---]": "",
    "dev": "tsup --watch",
    "dev:build": "tsup",
    "[--- Start ---]": "",
    "start": "node ./dist/index.js",
    "[--- Lint ---]": "",
    "lint": "tsc --noEmit --project tsconfig.dev.json && pnpm eslint ./src --ext .js,.ts,.tsx",
    "[--- Clean ---]": "",
    "clean": "rimraf dist node_modules",
    "[--- Test ---]": "",
    "test": "pnpm test:unit",
    "test:watch": "pnpm test:unit:watch",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "[--- Utils ---]": "",
    "ts": "node -r esbuild-register",
    "size": "pnpm run size-limit",
    "[--- Prisma ---]": "",
    "prisma:generate": "npx rimraf ./src/trpc && prisma generate --schema=./src/backend/schema.prisma",
    "prisma:migrate": "prisma migrate",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push --skip-generate"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@trpc/server": "^10.7.0",
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "prisma": "^4.8.1",
    "superjson": "1.12.1",
    "trpc-shield": "^0.3.0",
    "zod": "^3.20.2",
    "@oxygen/trpc-generator": "*"
  },
  "devDependencies": {
    "@oxygen/json-to-js": "*",
    "@oxygen/trpc-generator": "*",
    "prisma-docs-generator": "^0.5.0",
    "zod-prisma": "^0.5.4",
    "@oxygen/rollup-swc-config": "*"
  }
}
