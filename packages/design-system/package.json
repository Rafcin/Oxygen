    "loadjs": "^4.2.0",
    "lodash": "4.17.21",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-cool-dimensions": "^3.0.0",
    "react-cool-inview": "^3.0.1",
    "react-cool-onclickoutside": "^1.7.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.3",
    "react-is": "18.2.0",
    "react-use": "^17.4.0",
    "tslib": "2.4.1",
    "tss-react": "^4.5.1",
    "use-places-autocomplete": "^4.0.0",
    "uuid": "^9.0.0",
    "yup": "^0.32.11",
    "zustand": "^4.1.5",
    "@oxygen/json-to-js": "*"
  },
