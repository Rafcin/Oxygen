{
  "private": false,
  "license": "Apache-2.0",
  "version": "0.0.0",
  "name": "@oxygen/design-system",
  "description": "Design System by Rafcin",
  "author": {
    "name": "Raf Szuminski",
    "email": "rafcin.s@gmail.com",
    "url": "https://rafszuminski.com"
  },
  "repository": "rafcin/fox.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "design system",
    "mui"
  ],
  "engines": {
    "node": "^14.13.1 || >=16.0.0",
    "pnpm": ">=7"
  },
  "main": "./dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./internal": {
      "import": "./dist/esm/internal.js",
      "require": "./dist/internal.js"
    },
    "./styles.css": "./dist/styles.css",
    "./primitives.json": "./dist/primitives.json"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "files": [
    "dist",
    "internal",
    "README.md"
  ],
  "scripts": {
    "[--- Build ---]": "",
    "prebuild": "rimraf dist",
    "build": "pnpm run build:ts",
    "theme:build": "pnpm js:theme && pnpm dts:theme && pnpm run build:ts",
    "build:ts": "rollup --config",
    "[--- Dev ---]": "",
    "dev": "tsup --watch",
    "dev:build": "tsup",
    "[--- Lint ---]": "",
    "lint": "tsc --noEmit --project tsconfig.dev.json && pnpm eslint ./src --ext .js,.ts,.tsx",
    "[--- Clean ---]": "",
    "clean": "rimraf dist node_modules",
    "[--- Storybook ---]": "",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook --docs",
    "storybook:preview": "serve storybook-static",
    "[--- Test ---]": "",
    "test": "pnpm test:unit",
    "test:watch": "pnpm test:unit:watch",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "[--- Utils ---]": "",
    "ts": "node -r esbuild-register",
    "size": "pnpm run size-limit",
    "js:theme": "pnpm json-to-js -i ./src/core/theme/theme.json -o ./src/core/theme/theme.ts --ts",
    "dts:theme": "quicktype --top-level Theme -l typescript ./src/core/theme/theme.json -o ./src/declarations/theme/index.ts && ./scripts/prefix.sh ./src/declarations/theme/index.ts Oxygen"
  },
  "dependencies": {
