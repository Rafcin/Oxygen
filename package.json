{
  "private": true,
  "license": "MIT",
  "name": "@oxygen/repo",
  "version": "0.0.0",
  "author": {
    "name": "Raf Szuminski",
    "email": "rafcin.s@gmail.com",
    "url": "https://rafszuminski.com"
  },
  "repository": "rafcin/fox.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "type": "module",
  "scripts": {
    "[-- Build --]": "",
    "airbnb:build": "turbo run build --filter=@oxygen/airbnb",
    "docs:build": "turbo run build --filter=@oxygen/docs",
    "bot:build": "turbo run build --filter=@oxygen/bot",
    "sandbox:build": "turbo run build --filter=@oxygen/sandbox",
    "build": "turbo run build",
    "[-- Dev --]": "",
    "airbnb:dev": "turbo run dev --scope=@oxygen/airbnb --no-cache --parallel --continue --include-dependencies",
    "docs:dev": "turbo run dev --scope=@oxygen/docs --no-cache --parallel --continue --include-dependencies",
    "bot:dev": "turbo run dev --scope=@oxygen/bot --no-cache --parallel --continue --include-dependencies",
    "sandbox:dev": "turbo run dev --scope=@oxygen/sandbox --no-cache --parallel --continue --include-dependencies",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "[-- Clean --]": "",
    "clean": "turbo run clean && rimraf node_modules",
    "clean:install": "turbo run clean && ./scripts/clean.sh",
    "setup": "pnpm install && pnpm build",
    "refresh": "pnpm clean && pnpm setup",
    "[-- Deduplicate --]": "",
    "deduplicate": "npx pnpm-deduplicate",
    "deduplicate:check": "npx pnpm-deduplicate --list",
    "[-- Format --]": "",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "[-- Upgrade --]": "",
    "update-dependencies": "turbo update-dependencies && npx npm-check-updates -u"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "@trpc/client": "^10.1.0",
    "@trpc/next": "^10.1.0",
    "@trpc/react-query": "^10.1.0",
    "@trpc/server": "^10.1.0",
    "concurrently": "^7.3.0",
    "cpy-cli": "^4.2.0",
    "superjson": "^1.9.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "commitizen": "^4.2.5",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "is-ci": "^3.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "turbo": "^1.6.3",
    "typescript": "^4.9.3",
    "eslint": "^8.28.0",
    "rimraf": "^3.0.2",
    "quicktype": "15.1.0",
    "dts-gen": "0.6.1",
    "@types/cors": "^2.8.12",
    "@types/lodash": "^4.14.191",
    "@types/uuid": "^9.0.0",
    "@types/object-hash": "3.0.0",
    "@types/node": "^18.11.10",
    "@types/is-ci": "^3.0.0",
    "@types/loadjs": "^4.0.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9"
  }
}
